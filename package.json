{
  "name": "drupal-twig-extensions",
  "version": "1.0.0-alpha.6",
  "description": "JavaScript implementation of Drupalâ€™s Twig extensions",
  "author": "John Albin Wilkins <virtually.johnalbin@gmail.com> (https://john.albin.net/)",
  "homepage": "https://github.com/JohnAlbin/drupal-twig-extensions",
  "repository": "https://github.com/JohnAlbin/drupal-twig-extensions.git",
  "license": "(MIT OR GPL-2.0-only)",
  "keywords": [
    "Drupal",
    "Twig",
    "Twing"
  ],
  "files": [
    "lib/"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./twig": "./lib/twig.js",
    "./twing": "./lib/twing.js",
    "./filters/twig": "./lib/filters/twig.js",
    "./filters/twing": "./lib/filters/twing.js",
    "./functions/twig": "./lib/filters/twig.js",
    "./functions/twing": "./lib/filters/twing.js",
    "./helpers/twig": "./lib/helpers/twig.js",
    "./helpers/twing": "./lib/helpers/twing.js"
  },
  "imports": {
    "#twig": "./lib/twig.js",
    "#twing": "./lib/twing.js",
    "#twig-fixture": "./tests/fixtures/twig-helpers.js",
    "#twing-fixture": "./tests/fixtures/twing-helpers.js"
  },
  "type": "module",
  "scripts": {
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta",
    "release:rc": "standard-version --prerelease rc",
    "release:inspect": "npx npm-packlist",
    "lint": "eslint lib/ tests/",
    "lint:fix": "eslint --fix lib/ tests/",
    "format": "npm run lint:fix && npm run prettier",
    "format:check": "npm run lint && npm run prettier:check",
    "prettier": "prettier --write \"{.*/**/*,*,**/*}.{js,jsx,ts,tsx,json,md,yml,css,scss}\"",
    "prettier:check": "prettier --check \"{.*/**/*,*,**/*}.{js,jsx,ts,tsx,json,md,yml,css,scss}\"",
    "pretest": "npm run format:check",
    "test": "c8 ava"
  },
  "dependencies": {
    "drupal-attribute": "^1.0.2",
    "locutus": "^2.0.15",
    "lodash.clonedeep": "^4.5.0"
  },
  "optionalPeerDependencies": {
    "twig": "^1.15.4",
    "twing": "^5.0.2"
  },
  "devDependencies": {
    "ava": "^4.0.0-rc.1",
    "c8": "^7.10.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "twig": "^1.15.4",
    "twing": "^5.1.0"
  },
  "c8": {
    "all": true,
    "include": [
      "lib",
      "twig.js",
      "twing.js"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "reporter": [
      "text-summary",
      "lcov"
    ]
  }
}
