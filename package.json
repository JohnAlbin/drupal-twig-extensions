{
  "name": "drupal-twig-shim",
  "version": "1.x",
  "description": "JavaScript implementation of Drupal's Twig extensions",
  "main": "index.js",
  "files": [
    "filters",
    "functions",
    "index.js"
  ],
  "scripts": {
    "test": "mocha --reporter spec",
    "posttest": "xo --space=2 --no-esnext --no-semicolon --global=it --global=describe"
  },
  "keywords": [
    "Drupal",
    "Twig"
  ],
  "author": "John Albin Wilkins <virtually.johnalbin@gmail.com> (http://john.albin.net/)",
  "license": "(MIT OR GPL-2.0)",
  "devDependencies": {
    "mocha": "^8.3.1",
    "xo": "*"
  },
  "dependencies": {
    "object-keys": "^1.1.1",
    "twig": "^1.15.4"
  },
  "xo": {
    "rules": {
      "unicorn/filename-case": 0
    }
  }
}
